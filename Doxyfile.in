PROJECT_NAME           = "MDStressLab++"
MARKDOWN_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
EXTRACT_ALL            = YES
OUTPUT_DIRECTORY       = docs
INPUT                  = ./src/ ./include/ ./tests/ ./visualization/  ./mainpage.txt ./images/ 
IMAGE_PATH             = ./images/
EXAMPLE_PATH           = ./unit_tests/  ./tests/
EXCLUDE                = ./include/MDStressLab++/Eigen/
USE_MATHJAX            = YES
MATHJAX_VERSION        = MathJax_2
MATHJAX_FORMAT         = HTML-CSS
MATHJAX_RELPATH        = https://cdn.jsdelivr.net/npm/mathjax@2
SEARCH_INCLUDES        = YES
EXTRACT_STATIC         = YES
RECURSIVE              = YES
SOURCE_BROWSER         = YES
HTML_COLORSTYLE        = AUTO_LIGHT
GENERATE_TREEVIEW      = YES
EXTENSION_MAPPING = py=Python
FILE_PATTERNS = *.cpp *.h *.hpp *.py *.dox
EXTENSION_MAPPING = py=Python
OPTIMIZE_OUTPUT_JAVA = NO
OPTIMIZE_OUTPUT_PYTHON = YES

